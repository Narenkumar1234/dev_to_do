{
    "name": "dev-tab-extension",
    "version": "1.0.0",
    "description": "Custom Chrome new tab for daily dev tasks and meeting notes",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext .ts,.tsx --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "check": "tsc --noEmit",
        "clean": "rm -rf node_modules dist package-lock.json && echo 'Project cleaned. Run npm install again.'",
        "start": "npm run dev"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@tailwindcss/postcss": "^4.1.6",
        "dayjs": "^1.11.13",
        "quill": "^2.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-quill": "^2.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@vitejs/plugin-react": "^4.0.0",
        "autoprefixer": "^10.4.14",
        "postcss": "^8.4.21",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.3.3",
        "vite": "^5.0.0"
    }
}
