{
    "name": "dev-tab-extension",
    "version": "1.0.0",
    "description": "Custom Chrome new tab for daily dev tasks and meeting notes",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext .ts,.tsx --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "check": "tsc --noEmit",
        "clean": "rm -rf node_modules dist package-lock.json && echo 'Project cleaned. Run npm install again.'",
        "start": "npm run dev"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@tailwindcss/postcss": "^4.1.6",
        "@tiptap/extension-link": "^2.24.2",
        "@tiptap/react": "^2.24.2",
        "@tiptap/starter-kit": "^2.24.2",
        "dayjs": "^1.11.13",
        "lucide-react": "^0.525.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.27.1",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@typescript-eslint/eslint-plugin": "^8.32.0",
        "@typescript-eslint/parser": "^8.32.0",
        "@vitejs/plugin-react": "^4.0.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^9.27.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.4.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "postcss": "^8.4.21",
        "prettier": "^3.5.3",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.3.3",
        "vite": "^5.0.0"
    }
}
